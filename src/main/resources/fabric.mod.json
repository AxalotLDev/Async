{
  "schemaVersion": 1,
  "id": "async",
  "version": "${version}",
  "name": "Async",
  "description": "Async - Minecraft Entity Multi-Threading Mod",
  "authors": [
    "Grider",
    "AxalotL",
    "FurryMileon"
  ],
  "contact": {
    "sources": "https://github.com/AxalotLDev/Async",
    "issues": "https://github.com/AxalotLDev/Async/issues",
    "homepage": "https://modrinth.com/mod/async"
  },
  "license": "GPLv3",
  "icon": "assets/async/icon.png",
  "environment": "*",
  "entrypoints": {
    "preLaunch": [
      "com.axalotl.async.parallelised.fastutil.sync.SyncFuTransformer"
    ],
    "main": [
      "com.axalotl.async.Async"
    ]
  },
  "accessWidener": "async.accesswidener",
  "mixins": [
    "async.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.21.x",
    "java": ">=21",
    "cloth-config2": ">=5.0.34"
  },
  "custom": {
    "lithium:options": {
      "world.chunk_access": false,
      "entity": false,
      "collections": false,
      "alloc.entity_tracker": false,
      "chunk.entity_class_groups": false,
      "util.entity_movement_tracking": false,
      "util.entity_section_position": false,
      "mixin.experimental.entity": false
    }
  },
  "breaks": {
    "vmp": "*"
  },
  "suggests": {
  }
}
